{
  "name": "fylgja",
  "version": "1.2.6",
  "description": "The modular CSS framework. Powered via CSS components",
  "homepage": "https://fylgja.dev/",
  "repository": "https://github.com/fylgja/fylgja",
  "bugs": "https://github.com/fylgja/fylgja/issues",
  "author": "Fylgja",
  "license": "MIT",
  "main": "index.scss",
  "unpkg": "fylgja.css",
  "style": "fylgja.css",
  "keywords": [
    "fylgja",
    "css",
    "sass",
    "framework",
    "modular"
  ],
  "files": [
    "*.{css,scss}",
    "bundles/*.{css,scss}"
  ],
  "scripts": {
    "start": "npm run dev",
    "test:css": "npx stylelint components/**/*.scss",
    "test": "npm run test:css",
    "dev:playground": "npx http-server -s -o",
    "dev": "npm run dev:playground",
    "build:bundles": "npx sass --no-source-map -s compressed -I node_modules bundles:./",
    "build:main": "npx sass index.scss fylgja.css --no-source-map -s compressed -I node_modules",
    "build": "npm run build:bundles && npm run build:main",
    "prebuild": "npm test"
  },
  "dependencies": {
    "@fylgja/aria-only": "^1.2.3",
    "@fylgja/aspect-ratio": "^1.2.3",
    "@fylgja/auto-grid": "^1.1.3",
    "@fylgja/avatar": "^1.1.1",
    "@fylgja/badge": "^1.1.1",
    "@fylgja/base": "^1.1.1",
    "@fylgja/borders": "^1.0.0",
    "@fylgja/breadcrumbs": "^1.1.1",
    "@fylgja/button": "^1.2.7",
    "@fylgja/card": "^1.2.1",
    "@fylgja/code": "^1.0.3",
    "@fylgja/colors": "^1.1.0",
    "@fylgja/container": "^1.1.4",
    "@fylgja/control": "^1.2.1",
    "@fylgja/details": "^1.0.3",
    "@fylgja/dialog": "^1.3.2",
    "@fylgja/easing": "^1.1.1",
    "@fylgja/elevation": "^1.0.1",
    "@fylgja/fonts": "^1.0.0",
    "@fylgja/form": "^1.1.3",
    "@fylgja/gradient": "^1.1.1",
    "@fylgja/hashlink": "^1.1.0",
    "@fylgja/input-group": "^1.2.1",
    "@fylgja/list": "^1.1.1",
    "@fylgja/menu": "^1.3.1",
    "@fylgja/mq": "^1.2.1",
    "@fylgja/pagination": "^1.1.2",
    "@fylgja/print": "^1.0.3",
    "@fylgja/progress": "^1.1.2",
    "@fylgja/range": "^1.1.1",
    "@fylgja/scroll-slider": "^1.1.1",
    "@fylgja/scrollbar": "^1.1.1",
    "@fylgja/section": "^1.1.0",
    "@fylgja/shadow": "^1.1.0",
    "@fylgja/sizes": "^1.1.0",
    "@fylgja/stretched-link": "^1.1.2",
    "@fylgja/table": "^1.1.2",
    "@fylgja/theme": "^1.2.0",
    "@fylgja/transform": "^1.2.1",
    "@fylgja/transition": "^1.1.1",
    "@fylgja/utilpack": "^1.2.0",
    "@fylgja/z-layer": "^1.1.0"
  },
  "devDependencies": {
    "@fylgja/stylelint-config": "^5.0.3",
    "sass": "^1.58.0",
    "stylelint": "^14.16.1"
  }
}
