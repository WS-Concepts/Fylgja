  },
  "color-blue": {
    "$value": "#00f",
    "$type": "color"
  },
  "shadow-color": {
    "$value": "gray",
    "$type": "color"
  },
  "shadow-weight": {
    "$value": "1%"
  },
  "shadow-1": {
    "$value": [
      "0 2px 1px -1px hsl(var(--shadow-color) / calc(var(--shadow-weight) + 18%))",
      "0 1px 1px hsl(var(--shadow-color) / calc(var(--shadow-weight) + 12%))",
      "0 1px 3px hsl(var(--shadow-color) / calc(var(--shadow-weight) + 10%))"
    ]
  },
  "shadow-@media:dark-color": {
    "$value": "hsl(220 40% 2%)",
    "$type": "color"
  },
  "shadow-@media:dark-weight": {
    "$value": "25%"
  },
  "spin-@": {
    "$value": "@keyframes spin { to { rotate: 360deg }}"
  },
  "animateSpin": {
    "$value": "spin 1s linear infinite"
  }
}