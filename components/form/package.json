{
  "name": "@fylgja/form",
  "version": "1.1.2",
  "description": "The form component is super easy to use. Since it is a zero config module that styles the form elements directly",
  "homepage": "https://fylgja.dev/",
  "author": "Fylgja",
  "license": "MIT",
  "style": "form.css",
  "sass": "_index.scss",
  "unpkg": "form.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/fylgja/fylgja",
    "directory": "components/form"
  },
  "bugs": "https://github.com/fylgja/fylgja/issues",
  "keywords": [
    "fylgja",
    "fylgja component",
    "form"
  ],
  "scripts": {
    "build:legacy": "cp box.css style-box.css && cp field.css style-field.css",
