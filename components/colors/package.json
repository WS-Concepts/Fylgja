{
  "name": "@fylgja/colors",
  "version": "1.0.8",
  "description": "Add a collection of prebuild colors to any project with ease, using Fylgja Colors",
  "homepage": "https://fylgja.dev/",
  "author": "Fylgja",
  "license": "MIT",
  "type": "module",
  "main": "index.cjs",
  "unpkg": "colors.css",
  "style": "colors.css",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.cjs"
    },
    "./hsl": {
      "import": "./hsl.js",
      "require": "./hsl.cjs",
      "default": "./hsl.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fylgja/fylgja",
    "directory": "components/colors"
  },
  "bugs": "https://github.com/fylgja/fylgja/issues",
  "keywords": [
    "fylgja",
    "fylgja component",
    "props",
    "colors",
    "color-tokens"
  ],
  "scripts": {
    "build:hsl:map": "mkdir -p temp && node src/create-hsl-json.js",
    "build": "node _build.js"
  },
  "devDependencies": {
    "@fylgja/props-builder": "^1.0.2"
  }
}
