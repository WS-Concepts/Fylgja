{
  "name": "@fylgja/shadow",
  "version": "1.1.0",
  "description": "The Fylgja shadow offers Design Tokens (CSS props) for consistent and semantic shadows",
  "homepage": "https://fylgja.dev/",
  "author": "Fylgja",
  "license": "MIT",
  "type": "module",
  "main": "index.cjs",
  "unpkg": "shadow.css",
  "style": "shadow.css",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.cjs"
    },
    "./props": {
      "import": "./shadow.js",
      "require": "./shadow.cjs",
      "default": "./shadow.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fylgja/fylgja",
    "directory": "components/shadow"
  },
  "bugs": "https://github.com/fylgja/fylgja/issues",
  "keywords": [
    "fylgja",
    "fylgja component",
    "props",
    "shadow"
  ],
  "scripts": {
    "build": "node _build.js",
    "postbuild": "cp shadow.static.css safe.css"
  },
  "devDependencies": {
    "@fylgja/props-builder": "^1.0.8"
  }
}
