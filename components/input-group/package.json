{
  "name": "@fylgja/input-group",
  "version": "1.3.0",
  "description": "The input group extends upon the `@fylgja/form`. By providing a wrapper that allows for more complex form field styles",
  "homepage": "https://fylgja.dev/",
  "author": "Fylgja",
  "license": "MIT",
  "style": "input-group.css",
  "sass": "_index.scss",
  "unpkg": "input-group.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/fylgja/fylgja",
    "directory": "components/input-group"
  },
  "bugs": "https://github.com/fylgja/fylgja/issues",
  "keywords": [
    "fylgja",
    "fylgja component",
    "input-group"
  ],
  "scripts": {
    "build:legacy": "cp box.css style-box.css && cp field.css style-field.css",
    "build": "npx sass --no-source-map -s compressed -I node_modules src:./",
    "postbuild": "npm run build:legacy"
  },
  "dependencies": {
    "@fylgja/form": "^1.1.6",
    "@fylgja/theme": "^1.2.0"
  }
}
