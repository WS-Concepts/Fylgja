{
  "name": "@fylgja/fonts",
  "version": "1.0.1",
  "description": "The Fylgja fonts offers Design Tokens (CSS props) for consistent and semantic typography.",
  "homepage": "https://fylgja.dev/",
  "author": "Fylgja",
  "license": "MIT",
  "type": "module",
  "main": "index.cjs",
  "unpkg": "fonts.css",
  "style": "fonts.css",
  "module": "index.js",
  "exports": {
    ".": {
      "style": "./fonts.css",
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.cjs"
    },
    "./jit": {
      "import": "./jit-props.js",
      "require": "./jit-props.cjs",
      "default": "./jit-props.cjs"
    },
    "./host": "./fonts.host.css",
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fylgja/fylgja",
    "directory": "components/fonts"
  },
  "bugs": "https://github.com/fylgja/fylgja/issues",
  "keywords": [
    "fylgja",
    "fylgja component",
    "props",
    "fonts"
  ],
  "scripts": {
    "build": "node _build.js"
  },
  "devDependencies": {
    "@fylgja/props-builder": "^1.0.5"
  }
}
