{
  "name": "@fylgja/tokens",
  "version": "1.0.0",
  "description": "A collection of all Fylgja tokens offered for specific design tools or frameworks.",
  "homepage": "https://fylgja.dev/",
  "author": "Fylgja",
  "license": "MIT",
  "type": "module",
  "main": "index.cjs",
  "unpkg": "tokens.json",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.cjs"
    },
    "./jit": {
      "import": "./jit-props.js",
      "require": "./jit-props.cjs",
      "default": "./jit-props.cjs"
    },
    "./tailwind": {
      "import": "./tailwind-props.js",
      "require": "./tailwind-props.cjs",
      "default": "./tailwind-props.cjs"
    },
    "./json": "./tokens.json",
    "./tokens": "./tokens.json",
    "./figma": "./figma-tokens.json",
    "./figma-sync": "./figma-tokens.sync.json",
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fylgja/fylgja",
    "directory": "components/tokens"
  },
  "bugs": "https://github.com/fylgja/fylgja/issues",
  "keywords": [
    "fylgja",
    "fylgja component",
    "props"
  ],
  "scripts": {
    "build": "node _build.js"
  },
  "devDependencies": {
    "@fylgja/aspect-ratio": "^1.2.3",
    "@fylgja/borders": "^1.0.1",
    "@fylgja/colors": "^1.1.0",
    "@fylgja/easing": "^1.1.1",
    "@fylgja/fonts": "^1.0.0",
    "@fylgja/mq": "^1.2.2",
    "@fylgja/props-builder": "^1.0.7",
    "@fylgja/shadow": "^1.1.0",
    "@fylgja/sizes": "^1.1.0",
    "@fylgja/z-layer": "^1.1.0"
  }
}
